menu "GPS CSV logger"

config AT_MAGPIO
	string "MAGPIO command"
	default "AT\%XMAGPIO=1,0,0,1,1,1574,1577" if BOARD_NRF9160_PCA10090NS
	default "AT\%XMAGPIO=1,1,1,7,1,746,803,2,698,748,2,1710,2200,3,824,894,4,880,960,5,791,849,7,1574,1577" if BOARD_NRF9160_PCA20035NS
	help
	  MAGPIO command to run while setting up GPS

config AT_COEX0
	string "COEX0 command"
	default "AT\%XCOEX0=1,1,1570,1580" if BOARD_NRF9160_PCA10090NS
	default "AT\%XCOEX0=1,1,1570,1580"
	help
	  COEX0 command to run while setting up GPS

endmenu

menu "Zephyr Kernel"
source "$ZEPHYR_BASE/Kconfig.zephyr"
endmenu
